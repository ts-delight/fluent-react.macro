{
  "name": "@ts-delight/fluent-react.macro",
  "version": "1.0.0-alpha.3",
  "description": "Zero-overhead fluent type-safe alternative to JSX and React.createElement",
  "main": "fluent-react.macro.js",
  "types": "fluent-react.macro.d.ts",
  "scripts": {
    "format": "prettier --write ./**/*.js ./**/*.ts",
    "test": "tsc && jest"
  },
  "publishConfig": {
    "access": "public"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "keywords": [
    "type-safe",
    "react",
    "babel-plugin-macros"
  ],
  "author": "Lorefnon <lorefnon@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.2",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "babel-jest": "^24.9.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@babel/traverse": "^7.5.5",
    "babel-plugin-macros": "^2.6.1",
    "@babel/code-frame": "^7.5.5",
    "debug": "^4.1.1"
  }
}
